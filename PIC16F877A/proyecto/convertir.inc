CBLOCK .50
	NUMBER
	CEN
	DEC
	UNI
ENDC

BIT8_CDU
	MOVF NUMBER,F
	BTFSS STATUS,Z
	GOTO NUMBER_IS_NOT_ZERO

NUMBER_IS_NOT_ZERO
EVAL_DATO_MAY_IGUAL_10
	MOVLW D'10'
	SUBWF NUMBER,W
	BTFSC STATUS,C
	GOTO DATO_MAY_IGUAL_10
	GOTO DATO_MENOR_10
DATO_MENOR_10
	MOVF NUMBER,W
	MOVWF UNI
	RETURN
DATO_MAY_IGUAL_10
	MOVLW D'10'
	SUBWF NUMBER,F
	INCF DEC,F
	
	; EVALUA SI DEC=10
	MOVLW D'10'
	SUBWF DEC,W
	BTFSS STATUS,Z
	GOTO EVAL_DATO_MAY_IGUAL_10
	CLRF DEC
	INCF CEN,F
	GOTO EVAL_DATO_MAY_IGUAL_10